{
  "expo": {
    "name": "Softiel Client Hub",
    "slug": "softiel-client-hub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "softiel",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0056b8"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.softiel.clienthub"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0056b8"
      },
      "package": "com.softiel.clienthub"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "name": "Softiel Client Hub",
      "shortName": "Client Hub",
      "lang": "tr",
      "scope": "/",
      "themeColor": "#0056b8",
      "backgroundColor": "#f8f9fb",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/",
      "description": "Softiel müşterilerinin devam eden projelerini takip edebileceği platform",
      "dir": "ltr",
      "preferRelatedApplications": false
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}

